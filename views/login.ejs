  <form action="/login" method="POST" class="col s12">
    <div class="row">
      <div class="input-field col s12">
        <input id="email" type="email" name="email" class="validate">
        <label for="email">Email</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12 ">
        <input id="password" type="password" name="password" class="validate">
        <label for="password">Password</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field center col s12">
        <button class="waves-effect waves-light btn-large">
          Login
        </button>
      </div>
    </div>
    <h6 class="center">OR</h6>
    <div class="row">
      <div class="input-field  center col s12 m12 l6">
        <a class="waves-effect waves-light btn-large grey darken-4" href="/auth/github" >
          <i class="material-icons left">
              <img src="/images/Github-Mark.png" alt="github-icon" class="button-image">
          </i>
          Github
        </a>
      </div>
      <div class="input-field  center col s12 m12 l6 ">
        <a class="waves-effect waves-light btn-large indigo darken-1" href="/auth/linkedin">
          <i class="material-icons left">
            <img src="/images/Linked-in-logo.png" class="linkedin-image" alt="linkedin-icon" >
          </i>
          LinkedIn
        </a>
      </div>
    </div>
  </form>
    